    if ($state[$p:error]) then
      $state
    else if ($state[$p:l1] = 35) then                       (: '>>' :)
      let $state := p:consume(35, $input, $state)           (: '>>' :)
      return $state
    else
      let $state := p:consume(30, $input, $state)           (: '<<' :)
      return $state
