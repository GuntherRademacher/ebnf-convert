        return $state
    let $state := p:lookahead1(17, $input, $state)          (: CharCode | Char | CharRange | CharCodeRange | ']' :)
    return
      if ($state[$p:l1] = 39) then                          (: ']' :)
        $state
      else
        p:parse-CharClass-1($input, $state)
};

